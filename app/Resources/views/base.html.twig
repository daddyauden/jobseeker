<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8" />
  <meta content="yes" name="apple-mobile-web-app-capable" />
  <meta content="yes" name="apple-touch-fullscreen" />
  <meta content="telephone=no,email=no" name="format-detection" />
  <meta name="App-Config" content="fullscreen=yes,useHistoryState=yes,transition=yes" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="description" content="{{ " seo_description "|system }}" />
  <meta name="keywords" content="{{ " seo_keyword "|system }}" />
  <title>{{ "seo_title"|system }}{% block title_suffix %}{% endblock %}</title>
  <link rel="apple-touch-icon" href="{{ asset(" /apple-touch-icon.png ") }}">
  <link rel="canonical" href="{{ " host "|system }}" />
  <link rel="shortcut icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
  <link href="{{ asset("assets/css/bootstrap.css ") }}" rel="stylesheet" />
  <link href="{{ asset("assets/css/bootstrap-theme.css ") }}" rel="stylesheet" />
  <link href="{{ asset("assets/css/bootstrap-plugin.css ") }}" rel="stylesheet" />
  <link href="{{ asset("assets/css/bootstrap-extend.css ") }}" rel="stylesheet" />
  <link href="{{ asset("assets/css/font-awesome.css") }}" rel="stylesheet"/>
  <link href="{{ asset("assets/css/common.css ") }}" rel="stylesheet" /> {% block header_css %}{% endblock %}
  <!--[if lt IE 9]>
  <script src="{{ asset("assets/js/html5shiv.js") }}"></script>
  <script src="{{ asset("assets/js/respond.js") }}"></script>
  <![endif]-->
  <script type="text/javascript">
    var locale = "{{ app.request.getLocale }}";
    var domain = "{{ "domain"|system }}";
  </script>
  {% block header_js %}{% endblock %}
</head>

<body class="main-background" style="background-image: url('/media/wallpaper/{{ "country"|system|lower }}/5.jpg');">
  <div id="main">
    <div id="main_inner">
      <div id="header">
        <div id="header_inner">
          {% block header_inner %}{% endblock %}
        </div>
      </div>
      <div id="container">
        <div id="container_inner">
          {% block container_inner %}{% endblock %}
        </div>
      </div>
      <div id="footer">
        <div id="footer_inner">
          {% block footer_inner %}{% endblock %}
        </div>
      </div>
    </div>
  </div>
</body>

<script src="{{ asset("assets/js/jquery.js ") }}"></script>
<script src="{{ asset("assets/js/jquery.plugin.js ") }}"></script>
<script src="{{ asset("assets/js/bootstrap.js ") }}"></script>
<script src="{{ asset("assets/js/bootstrap-plugin.js ") }}"></script>
<script src="{{ asset("assets/js/common.js ") }}"></script>
{% block footer_js %}{% endblock %}

</html>
